    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lity/2.4.1/lity.min.css">
    <link rel="preload" as="font" type="font/woff" crossorigin="anonymous" href="~/Content/HomeStyle/assets/fonts/Averta-Bold.woff">
    <link rel="preload" as="font" type="font/woff2" crossorigin="anonymous" href="~/Content/HomeStyle/assets/fonts/Averta-Bold.woff2">
